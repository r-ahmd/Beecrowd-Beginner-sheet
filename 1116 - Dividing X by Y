#include <stdio.h>
 
int main()
{
    int n, a,b;
    scanf("%d", &n);
    float result;
    while(n--)
    {
        scanf("%d %d", &a, &b);
        result = (float)a/b;
        if(b==0)
            printf("divisao impossivel\n");
        else
            printf("%.1f\n", result);
    }
    
 
    return 0;
}
