#include <stdio.h>
#include <math.h>
 
int main() 
{
    int n;
    scanf("%d", &n);
    if(n>5 && n<2000)
    {
        for(int i=1; i<=n; i++)
            if(i%2==0)
            {
                int result = pow(i,2);
                printf("%d^2 = %d\n", i, result);
            }
                
    }
 
    return 0;
}
