#include <stdio.h>
#include <string.h>
 
int main() 
{
    int n;
    char ch[35]; //char ch[35]="LIFE IS NOT A PROBLEM TO BE SOLVED"; //you can use this instead of strcpy..but it has longer runtime
    strcpy(ch, "LIFE IS NOT A PROBLEM TO BE SOLVED");
    scanf("%d", &n);
    while(n>=0 && n<=34)
    {
        for(int i=0; i<n; i++)
            printf("%c", ch[i]);
        printf("\n");
        break;
    }
 
    return 0;
}
