#include <stdio.h>

int main() 
{
    int x, y, a, b, count=1;
    scanf("%d %d", &x, &y);
    a = y/x;
    b = x;
    for(int i=1; i<=a; i++)
    {
       printf("%d", count);
        for(int j=count+1; j<=b; j++)
            printf(" %d", j);
        printf("\n");
        
        count+=x;
        b+=x;
    }
    
    return 0;
}
